{
    "name": "rockbuzz/lara-memberships",
    "description": "Abstraction for checkout implementation",
    "keywords": [
        "memberships"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "TiagoDevWeb",
            "email": "tiago.dweb@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rockbuzz\\LaraMemberships\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "bensampo/laravel-enum": "^1.0",
        "spatie/laravel-schemaless-attributes": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "cs": "vendor/bin/phpcs"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rockbuzz\\LaraMemberships\\ServiceProvider"
            ]
        }
    }
}
